{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",

  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },

    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/imgs/**",
          "/*.(png|jpg|jpeg|svg|webp)",
          "/*.(woff|woff2|ttf|otf)"
        ]
      }
    }
  ],

  "dataGroups": [
    {
      "name": "api-cache",
      "urls": [
        "https://api.servidor.com/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "1h",
        "timeout": "10s"
      }
    }
  ]
}